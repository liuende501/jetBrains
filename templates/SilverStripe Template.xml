<templateSet group="SilverStripe Template">
  <template name="if" value="&lt;% if $$$ifVar$ %&gt;&#10;    $END$&#10;&lt;% end_if %&gt;" description="An if block" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="ifVar" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="elseif" value="&lt;% else_if $$$ifVar$ %&gt;" description="An else if tag" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="ifVar" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="else" value="&lt;% else %&gt;" description="An else tag" toReformat="true" toShortenFQNames="true" deactivated="true">
    <context />
  </template>
  <template name="with" value="&lt;% with $$$withVar$ %&gt;&#10;    $END$&#10;&lt;% end_with %&gt;" description="A with block. Used to change scope." toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="withVar" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="loop" value="&lt;% loop $$$loopVar$ %&gt;&#10;    $END$&#10;  &lt;% end_loop %&gt;" description="A loop block. Used to loop over collections." toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="loopVar" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="cached" value="&lt;% cached $$$cachedVar$ %&gt;&#10;    $END$&#10;  &lt;% end_cached %&gt;" description="A cached block. Code within this block is cached." toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="cachedVar" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="inc" value="&lt;% include $incFile$ %&gt;" description="Include file from Includes dir." toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="incFile" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="reqcss" value="&lt;% require themedCSS('$incFile$') %&gt;" description="Require Theme CSS." toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="incFile" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="reqjs" value="&lt;% require javascript('$incFile$') %&gt;" description="Require JS." toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="incFile" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="base" value="&lt;% base_tag %&gt;" description="HTML Base tag." toReformat="true" toShortenFQNames="true" deactivated="true">
    <context />
  </template>
  <template name="_t" value="&lt;%t $identifier$ '$string$' %&gt;" description="Translation tag. Used with lang yaml files." toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="identifier" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="string" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
</templateSet>