    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <multiLine>false</multiLine>
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\bdebug\b.*" />
    <pattern pattern="\bltodo\b.*" />
    <filter name="ltodo">
      <pattern index="3" />
    </filter>
  </component>
</application>